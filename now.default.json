{
  "name": "naia",
  "alias": [
    "naia.now.sh"
  ],
  "github": {
    "enabled": true,
    "autoAlias": true
  },
  "builds": [
    { "src":  "index.js",
       "use": "@now/node-server"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "index.js" },
    { "src": "/ping/(.*)", "dest": "index.js" }
  ],
  "version": 2,
  "env": {
    "MONGO_USER": "",
    "MONGO_PASSWORD": "",
    "IMGUR_CLIENT_ID": "",
    "IMGUR_CLIENT_SECRET": "",
    "IMGUR_ACCOUNT":"",
    "IMGUR_PASSWORD": "",
    "AUTH0_DOMAIN": "",
    "AUTH0_AUDIENCE": "",
    "AUTH0_CLIENT": "",
    "AUTH0_SECRET": ""

  }
}
